<?xml version="1.0" encoding="UTF-8"?>

<openbox_config xmlns="http://openbox.org/3.4/rc"
		xmlns:xi="http://www.w3.org/2001/XInclude">

<keyboard>
	<!-- Keybindings for netmon GPIOs -->
	<keybind key="F5">
		<action name="Execute">
			<startupnotify>
				<enabled>true</enabled>
				<name>xterm</name>
			</startupnotify>
			<command>xterm</command>
		</action>
	</keybind>
</keyboard>

</openbox_config>
